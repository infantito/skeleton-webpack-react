{
  "name": "react-webpack2-skeleton",
  "version": "1.0.0",
  "description": "Get started with React with Webpack2",
  "main": "index.js",
  "keywords": [],
  "author": "Daniel Infante <danielinfantito61@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "start:prod": "cross-env BABEL_ENV=production && cross-env NODE_ENV=production",
    "start:dev": "cross-env BABEL_ENV=development && cross-env NODE_ENV=development",
    "start": "npm run start:dev && webpack-dev-server --config=config/webpack.development.js",
    "build": "npm run clean && npm run start:prod && webpack --display-error-details -p --config=config/webpack.production.js",
    "lint": "eslint src/"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-which-builtins": "^0.0.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "^6.23.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.1",
    "node-sass": "^4.5.0",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "next",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-recommended": "^1.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
